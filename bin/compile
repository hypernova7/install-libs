#!/usr/bin/env node
const { env } = process
const { exec } = require('child_process')
exec('npm i -g find-up', (e, out, err) => {
  if (e || err) return;
  const { sync } = require('find-up')
  console.log(sync('.env'))
  console.log(env.CACHE_DIR)
})
